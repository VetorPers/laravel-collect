{
  "name": "vetor/laravel-collect",
  "description": "laravel collect",
  "license": "MIT",
  "authors": [
    {
      "name": "VetorPers",
      "email": "506969463@qq.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "illuminate/database": "~5.5.0|~5.6.0|~5.7.0",
    "illuminate/support": "~5.5.0|~5.6.0|~5.7.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/database": "~3.5.0|~3.6.0|~3.7.0",
    "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0",
    "phpunit/phpunit": "^6.0|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Vetor\\Contracts\\Collect\\": "contracts/",
      "Vetor\\Laravel\\Collect\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vetor\\Tests\\Collect\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vetor\\Laravel\\Collect\\Providers\\CollectServiceProvider"
      ]
    }
  }
